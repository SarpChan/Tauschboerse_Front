<UserControl x:Class="Frontend.UserControls.Login_UsernameInput"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Frontend.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="80" d:DesignWidth="750">
    <UserControl.Resources>
        <!--Login UserInput-->
        <Style TargetType="{x:Type Grid}" x:Key="UsernameInput">
        </Style>
    </UserControl.Resources>
    
    
    <Grid Style="{StaticResource UsernameInput}" Tag="Username"/>
</UserControl>
