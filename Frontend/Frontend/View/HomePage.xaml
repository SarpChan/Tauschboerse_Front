<Page x:Class="Frontend.View.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Frontend.View"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1640"
      Title="HomePage">

    <Grid Width="1640" Height="1000" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="90"/>
            <ColumnDefinition Width="1550"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="77"/>
            <RowDefinition Height="113"/>
            <RowDefinition Height="137"/>
            <RowDefinition Height="593"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Column="1" HorizontalAlignment="Left" Grid.Row="1" TextWrapping="Wrap" Text="Home" VerticalAlignment="Top" FontSize="32" FontFamily="/Frontend;component/Resources/Fonts/#Kiona"/>

        <Canvas x:Name="Home_Canvas" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="1460" Grid.Column="1" Grid.Row="3" Background="#FFD2E7EB">
            <TextBlock Text="Home Stuff" Canvas.Left="622.5" Canvas.Top="10" FontSize="34" FontFamily="Kiona"/>
        </Canvas>


    </Grid>
</Page>
